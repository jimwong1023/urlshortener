<div class="container">
  <a href="/url"><h3>Create a shortened URL!</h3></a>
  <hr class="top" size="100">
     <% @urls.each do |url| %>
      <a href="<%="/#{url.key}"%>"><h4><%= url.key %></h4></a>
      <p>count: <%= url.count %></p>
      <p>original url: <%= url.original_url %></p>
      <hr>
  <% end %>
</div>
